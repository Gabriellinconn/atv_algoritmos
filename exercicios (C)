7)indice de par e impar
#include <stdio.h>
#include <locale>

int main(){
	setlocale (LC_ALL, "portuguese");
	
	int i, impar, par, vetor[5]={2,3,4,5,6} ;
	
	par=impar=0;
	
	for(i=0; i<5; i++){
		if(vetor[i]%2!=0){
			printf("%d ímpar \n", vetor[i]);
			impar++;
		}
		if(vetor[i]%2==0){
			printf("%d par \n", vetor[i]);
			par++;
		}
	}
	printf(" %d números ímpares E %d núemros pares", impar, par);
	
	
	
	return 0;
}
