6)
#include <stdio.h>
#include <locale.h>

main(){
	
	setlocale (LC_ALL, "portuguese");
	
int num, soma;
	
	soma=0;
	
	do {
		printf("Informe um número: ");
		scanf("%d", &num);
		
		soma= soma+num;
	}while(num!=0);
	
	printf("o resultado é: %d", soma);
		
	
	return 0;
}

