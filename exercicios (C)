14) calculo diminuindo e somando

#include <stdio.h>
#include <locale>

int main (){
	setlocale(LC_ALL, "portuguese");
	
	int i ;
	float numerador, denominador, resultado;
	
	numerador=1000;
	denominador=1;
	resultado=0;
	
	for(i=1; i<=25; i++){
		
		resultado+=numerador/denominador;
		
		numerador-=3;
		denominador+=1;
		
		resultado-=numerador/denominador;
		
		numerador-=3;
		denominador+=1;
		
	}
	
	printf("Resultado: %.2f",resultado);

	return 0;
}

