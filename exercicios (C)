VETORES 
1) vetores ao inverso
#include <stdio.h>
#include <locale>

main (){
	
	setlocale(LC_ALL, "portuguese");
	int vetor[5], i;
	
	for(i=0; i<5; i++){
		printf("Informe o valor do número %d: ", i+1);
		scanf("%d", &vetor[i]);
	}
	for(i=4; i>=0; i--){
		printf("%d ", vetor[i]);
	}
	
	return 0;
}

