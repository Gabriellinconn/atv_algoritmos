15)Fibonacci

#include <stdio.h>
#include <locale>

int main (){
	setlocale(LC_ALL, "portuguese");
	
	int num, i ,anterior, atual ;

	printf("insira número: ");
	scanf("%d", &num);

	anterior=1;
	atual=1;
	
	printf("1 1");
	
	for(i=1;i<=num-2;i++){
		atual= anterior+atual;
		anterior=atual-anterior;
		
		printf(" %d", atual);
	}

	return 0;
}

