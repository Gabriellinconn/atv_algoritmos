6) mostrar maior número

#include <stdio.h>
#include <locale.h>


int maior (int n1, int n2){
	if (n1>n2){
		return n1;
	}
	else {
		return n2;
	}
}


int main(){
	
	setlocale(LC_ALL, "portuguese");
	int num1, num2;
	
	printf("Digite Número 1: ");
	scanf("%d", &num1);
	
	printf("Digite Número 2: ");
	scanf("%d", &num2);
	
	printf("O maior número é: %d", maior (num1, num2));
	
	return 0;
}  
