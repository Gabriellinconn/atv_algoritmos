12) 100/1 a 1/100

#include <stdio.h>
#include <locale>

int main (){
	setlocale(LC_ALL, "portuguese");
	
	int i ;
	float numerador, denominador, soma;
	
	numerador=100;
	denominador=1;
	soma=0;
	
	for(i=1;i<=100;i++){
		soma= soma+ numerador/denominador;
		numerador-=1;
		denominador+=1;
	}
	
	printf("Resultado: %.2f", soma);
	
	return 0;
}

