2)
#include <stdio.h>
#include <locale>

main (){
	
	setlocale(LC_ALL, "portuguese");
	int vetor[5], i;
	
	for(i=0; i<5; i++){
		printf("Informe o valor do número %d: ", i+1);
		scanf("%d", &vetor[i]);
	}
	for(i=0; i<5; i++){
		if(vetor[i]%2!=0){
		
		printf("%d ", vetor[i]);
	}
	}

	return 0;
}

3)a media dos vetores e os vetores acima da média

#include <stdio.h>
#include <locale>

main (){
	
	setlocale(LC_ALL, "portuguese");
	int  i;
	float vetor[5], soma, media;
	
	soma=0;
	
	for(i=0; i<5; i++){
		printf("Informe o valor do número %d: ", i+1);
		scanf("%f", &vetor[i]);
		soma+=vetor[i];
	}
	media=soma/i;
	
	printf("A média dos valores são: %.1f\n", media);
		printf("Os valores dentro da média são: \n");

	
	for(i=0; i<5; i++){
		if(vetor[i]>=media){
			printf("%.1f   ", vetor[i]);
		}
	}
	
	

	return 0;
} 
