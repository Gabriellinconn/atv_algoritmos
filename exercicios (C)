#include <stdio.h>
#include <locale.h>

int main(){
	
	setlocale(LC_ALL, "portuguese");
	
	int matriz[2][3]={{1,2,3}, {4,5,6}}, i, j, soma;
	
	soma=0;
	
	for(i=0;i<2;i++){
		for(j=0;j<3;j++){
			soma= soma+ matriz[i][j];
		}
	}
	
	printf("Soma: %d", soma);
	
	return 0;
}

