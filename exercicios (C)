#include <locale.h>

main(){
	
	setlocale (LC_ALL, "portuguese");
	
int i, num;
	
	printf("insira um valor: ");
	scanf("%d", &num);
	


	
	for(i=2;i<=num;i+=2){
		
		
		printf("%d \n", i);
		
	}
		printf("%d números expressos", num/2);
		
	
	return 0;
}
