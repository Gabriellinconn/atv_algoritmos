int main() {

	setlocale(LC_ALL, "portuguese");

	int vetor[6] = {0, 0, 0, 0, 0, 0}, num, i;
	for(i = 1; i <= 15; i++) {
		printf("Informe número %d: ", i);
		scanf("%d", &num);
		vetor[num - 1]++;
	}
	for(i = 0; i < 6; i++) {
		printf("\nOcorrências face %d: %d", i + 1, vetor[i]);
	}
	return 0;
}

