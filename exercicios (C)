16) somatoria ate 100

#include <stdio.h>
#include <locale.h>

int main(){
	
	int num, i, vezes;
	
	num = 0;
	
	for(i=1; num<=100; i++){
		num+=i;
		
		printf("%d \n", num);
		
		vezes+=1;
	}

	
	printf ("\nForam somadas %d vezes", vezes);
	
	
	return 0;
	
}

