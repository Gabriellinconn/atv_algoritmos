2) fogo!
for - while atividades

#include <stdio.h>
#include <locale>
#include <windows.h>

int main(){
	
	int i;
	
	for (i=10; i>=1; i--){
		printf("%d\n", i);
		Sleep (1000);
	}
	printf("fogo!!");
	return 0;
	
}
