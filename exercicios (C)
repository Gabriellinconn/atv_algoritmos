	int i, vetor[5]={4,7,3,8,6}, num;
	
	printf("Informe um número para consulta se já existente: ");
	scanf("%d", &num);
	
	for(i=0;i<5;i++){
		if (num==vetor[i]){
			printf("O número (%d) informado foi encontrado no vetor %d", num, i);
		return 0;	
		}
		
	}
