7)o maior de 3 números

#include <stdio.h>
#include <locale.h>


int maior (int n1, int n2, int n3){
	int maior;
	maior = n1;
	if (maior<n2){
		maior=n2;
	}
	if(maior<n3) {
		maior=n3;
	}
	return maior;
}


int main(){
	
	setlocale(LC_ALL, "portuguese");
	int num1, num2, num3;
	
	printf("Digite Número 1: ");
	scanf("%d", &num1);
	
	printf("Digite Número 2: ");
	scanf("%d", &num2);
	
	printf("Digite Número 3: ");
	scanf("%d", &num3);
	
	printf("O maior número é: %d", maior (num1, num2, num3));
	
	return 0;
}

--------------------------------------------------------------------------

8)media ponderada ou aritmetica de 3 notas (com switch case)

#include <stdio.h>
#include <locale.h>


float media (float n1, float n2, float n3, char tipo){
	float nota;
switch (tipo){
	case 'A':
		nota = (n1+n2+n3)/3;
		break;
		
		case 'P':
			nota = (n1*5+n2*3+n3*2)/10;
			break;
}
return nota;
}


int main(){
	
	setlocale(LC_ALL, "portuguese");
	float num1, num2, num3;
	char tipo;
	
	printf("Digite Tipo (A ou P): ");
	scanf("%c", &tipo);
	
	printf("Digite nota 1: ");
	scanf("%f", &num1);
	
	printf("Digite nota 2: ");
	scanf("%f", &num2);
	
	printf("Digite nota 3: ");
	scanf("%f", &num3);
	
	printf("A média escolhida tem resultado: %.2f", media (num1, num2, num3, tipo));
	
	return 0;
}

----------------------------------------------------------------------------------

9) calculadora

#include <stdio.h>
#include <locale.h>


float conta (float n1, char op, float n2){
	float resultado;
switch (op){
	
			case '+':
			resultado = n1+n2;
			break;
		
			case '-':
			resultado = n1-n2;
			break;
			
			case '*':
			resultado = n1*n2;
			break;
			
			case '/':
			resultado = n1/n2;
			break;
}
return resultado;
}


int main(){
	
	setlocale(LC_ALL, "portuguese");
	float num1, num2;
	char op;
	
	
	
	printf("Digite numero 1: ");
	scanf("%f", &num1);
	
	printf("Digite uma operação (+,-,* ou /): ");
	scanf(" %c", &op);
	
	printf("Digite numero 2: ");
	scanf("%f", &num2);
	

	printf("A operação teve resultado: %.2f", conta (num1, op, num2));
	
	return 0;
}

10) escadinha com numero escolhido

#include <stdio.h>
#include <locale.h>


void linha (int num){
int i, j;

for(i=1;i<=num;i++){
	for(j=1;j<=i; j++){
		printf("*");
	}
	printf("\n");
}
}


int main(){
	
	setlocale(LC_ALL, "portuguese");
	int num;
	
	
	
	printf("Digite numero: ");
	scanf("%d", &num);
	

	linha(num);
	
	return 0;
}

