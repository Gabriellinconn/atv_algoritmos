17) amplitude

#include <stdio.h>
#include <locale.h>

int main(){
	
	setlocale(LC_ALL, "portuguese");
	
	float num, maior, menor;
	
	printf("Insira Número: ");
	scanf("%f", &num);
	
	menor=num;
	maior=num;
	
	while (num!=0){
		
	
		if(num>maior){
			maior=num;
		}
		if (num<menor){
			menor=num;	
		}
	printf("Insira Número: ");
		scanf("%f", &num);
		}
	
	printf("Resultado da amplitude: %.1f", maior-menor);
	
	return 0;
	
}

