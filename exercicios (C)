11)soma em escadinha

#include <stdio.h>
#include <locale.h>

int main() {

	setlocale (LC_ALL,"portuguese");

	int M[4][4]={
	{1,2,3,4},
	{6,7,8,9},
	{11,12,13,14},
	{16,17,18,19}}, i,j, soma;

	soma=0;

	for (i=0; i<4; i++) {
		for(j=0;j<4;j++){
			if(i>=j){
			soma+=M[i][j];
	}
		}
	
	}

	printf ("A soma dos Números na terceira linha é %d", soma);

	return 0;
}

12)escada invertida (nao entendi)
#include <stdio.h>
#include <locale.h>

int main() {

	setlocale (LC_ALL,"portuguese");

	int M[4][4]={
	{1,2,3,4},
	{6,7,8,9},
	{11,12,13,14},
	{16,17,18,19}}, i,j, soma;

	soma=0;

	for (i=0; i<4; i++) {
		for(j=3;j>=3-1;j--){
			if(i>=j){
			soma+=M[i][j];
	}
		}
	
	}

	printf ("A soma dos Números na terceira linha é %d", soma);

	return 0;
}

13)menores numeros de cada vetor dentro de uma matriz

#include <stdio.h>
#include <locale.h>

int main() {

	setlocale (LC_ALL,"portuguese");

	int M[4][5] = {{5, 4, 2, 8, 7}, {3, 6, 9, 1, 4}, {0, 3, 5, 2, 9}, {4, 9, 7, 6,2}}, V[4], i, j, menor;

	for(i = 0; i < 4; i++) {
		menor = M[i][0];

		for(j = 1; j < 5; j++) {

			if(M[i][j] < menor) {
				menor = M[i][j];
			}

		}
		V[i] = menor;
	}
	for(i = 0; i < 4; i++) {
		printf("%d ", V[i]);
	}
	return 0;
}

14) troca de matrizes com aux

#include <stdio.h>
#include <locale.h>

int main() {

	setlocale (LC_ALL,"portuguese");

	int A[2][3] = {{10, 20, 30}, {40, 50, 60}}, B[2][3] = {{1, 2, 3}, {4, 5, 6}}, i,j, aux;
	for(i = 0; i < 2; i++) {
		for(j = 0; j < 3; j++) {
			aux = A[i][j];
			A[i][j] = B[i][j];
			B[i][j] = aux;
		}
	}
	printf("Matriz A:\n");
	for(i = 0; i < 2; i++) {
		for(j = 0; j < 3; j++) {
			printf("%d\t", A[i][j]);
		}
		printf("\n");
	}
	printf("\nMatriz B:\n");
	for(i = 0; i < 2; i++) {
		for(j = 0; j < 3; j++) {
			printf("%d\t", B[i][j]);
		}
		printf("\n");
	}
	return 0;
}

