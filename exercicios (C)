1 e 2 e 3 kkkk) repetir *

#include <stdio.h>
#include <locale.h>

void linha (int quant){
	for(int i=0;i<quant; i++){
		printf("*");
	}
	printf("\n");
}

int main(){
	
setlocale(LC_ALL, "portuguese");

linha(20);

printf("Números de 1 a 5: \n");

linha(20);

for(int i=1;i<6; i++){
	printf(" %d", i);
	printf("\n");
}

linha(20);

return 0;
}

