    float P[10];
    
    for(i=0;i<10;i++){
                       printf("Quantas quantidades do produto %d foram vendidas?: \n", i);
                       scanf("%d", &Q[i]);
                   }
                       for (i=0;i<10;i++){
                            printf("Qual o preço do produto %d? ", i);
                            scanf("%f", &P[i]);
                        }
                            
                            for (i=0;i<10;i++){
                       faturamento+=Q[i]*P[i];
                            }
                            

 printf("O Fatoramento mensal é de %d", faturamento);

return 0;
}

