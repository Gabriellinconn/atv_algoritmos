13) 

#include <stdio.h>
#include <locale>

int main (){
	setlocale(LC_ALL, "portuguese");
	
	int i ;
	float numerador, denominador, resultado;
	
	numerador=3;
	denominador=2;
	resultado=1;
	
	for(i=1; i<=19; i++){
		
		resultado+=numerador/denominador;
		
		numerador+=2;
		denominador*=2;
		
		
	}
	
	printf("Resultado: %.2f",resultado);

	return 0;
}

